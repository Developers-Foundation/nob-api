{
  "name": "nobodyrandom/nob-api",
  "description": "Developers' Foundation Main Website",
  "minimum-stability": "stable",
  "homepage": "https://github.com/nobodyrandom/nob-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Harrison Chow",
      "email": "harrison@developersfoundation.ca",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.6|~7.0",
    "sendgrid/sendgrid": "4.0.4",
    "parse/php-sdk": "1.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "nobodyrandom\\nob-api\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      ":vendor\\:package_name\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
